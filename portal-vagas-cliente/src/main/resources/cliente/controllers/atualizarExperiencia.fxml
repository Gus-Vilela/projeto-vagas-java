<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.AtualizarExperiencia">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="220.0" layoutY="50.0" text="Atualizar Experiência">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="nomeInput" layoutX="185.0" layoutY="135.0" prefHeight="25.0" prefWidth="230.0" />
            <Label layoutX="192.0" layoutY="113.0" text="Nome da Competência:" />
            <Label layoutX="191.0" layoutY="180.0" text="Tempo:" />
            <Button fx:id="btnCadastrar" layoutX="268.0" layoutY="266.0" mnemonicParsing="false" onAction="#onClickCadastrar" text="Atualizar" />
            <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="166.0" layoutY="236.0" opacity="0.0" prefHeight="17.0" prefWidth="272.0" textAlignment="CENTER" textFill="RED" />
            <Button fx:id="btnVoltar" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onClickVoltar" text="Voltar" />
