<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.CadastrarCandidato">
   <children>
      <Label layoutX="219.0" layoutY="49.0" text="Cadastrar Candidato">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nomeInput" layoutX="185.0" layoutY="135.0" prefHeight="25.0" prefWidth="230.0" />
      <TextField fx:id="emailInput" layoutX="185.0" layoutY="193.0" prefHeight="25.0" prefWidth="230.0" />
      <TextField fx:id="senhaInput" layoutX="185.0" layoutY="252.0" prefHeight="25.0" prefWidth="230.0" />
      <Label layoutX="192.0" layoutY="111.0" text="Nome:" />
      <Label layoutX="192.0" layoutY="167.0" text="Email:" />
      <Label layoutX="190.0" layoutY="227.0" text="Senha:" />
      <Button fx:id="btnCadastrar" layoutX="267.0" layoutY="319.0" mnemonicParsing="false" onAction="#onClickCadastrar" text="Cadastrar" />
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="165.0" layoutY="291.0" opacity="0.0" prefHeight="17.0" prefWidth="272.0" textAlignment="CENTER" textFill="RED" />
      <Button fx:id="btnVoltar" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Voltar" onAction="#onClickVoltar"/>
   </children>
</AnchorPane>
