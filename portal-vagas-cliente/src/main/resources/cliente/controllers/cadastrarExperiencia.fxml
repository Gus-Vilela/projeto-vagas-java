<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.CadastrarExperiencia">
   <children>
      <Label layoutX="215.0" layoutY="48.0" text="Cadastrar Experiência">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nomeInput" layoutX="185.0" layoutY="135.0" prefHeight="25.0" prefWidth="230.0" />
      <Label layoutX="192.0" layoutY="113.0" text="Nome da Competência:" />
      <Label layoutX="191.0" layoutY="180.0" text="Tempo:" />
      <Button fx:id="btnCadastrar" onAction="#onClickCadastrar" layoutX="268.0" layoutY="266.0" mnemonicParsing="false"  text="Cadastrar" />
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="166.0" layoutY="236.0" opacity="0.0" prefHeight="17.0" prefWidth="272.0" textAlignment="CENTER" textFill="RED" />
      <Button fx:id="btnVoltar" onAction="#onClickVoltar" layoutX="14.0" layoutY="14.0" mnemonicParsing="false"  text="Voltar" />
      <Spinner fx:id="tempoInput" layoutX="185.0" layoutY="200.0" prefHeight="25.0" prefWidth="230.0" />
   </children>
</AnchorPane>
