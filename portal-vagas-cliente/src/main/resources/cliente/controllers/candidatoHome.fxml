      <Button fx:id="btnCadastrarExperiencia" layoutX="327.0" layoutY="136.0" mnemonicParsing="false" onAction="#onClickCadastrarExperiencia" prefHeight="25.0" prefWidth="137.0" text="Cadastrar Experiência" />
