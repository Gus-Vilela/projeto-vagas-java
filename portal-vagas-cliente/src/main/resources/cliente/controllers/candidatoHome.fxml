<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.CandidatoHome">
   <children>
      <Label layoutX="263.0" layoutY="79.0" text="Bem Vindo">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnMeuCadastro" layoutX="137.0" layoutY="136.0" mnemonicParsing="false" onAction="#onClickMeuCadastro" prefHeight="25.0" prefWidth="137.0" text="Meu cadastro" />
      <Button fx:id="btnEditarCadastro" layoutX="137.0" layoutY="190.0" mnemonicParsing="false" onAction="#onClickAtualizarCadastro" prefHeight="25.0" prefWidth="137.0" text="Editar meu cadastro" />
