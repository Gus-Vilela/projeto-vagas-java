<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.ListarVagas">

    <TableView fx:id="vagasTable" layoutX="20.0" layoutY="59.0" prefHeight="327.0" prefWidth="560.0">
        <columns>
            <TableColumn fx:id="idVagaColumn" prefWidth="100.0" text="ID Vaga" />
            <TableColumn fx:id="nomeColumn" prefWidth="200.0" text="Nome" />
            <TableColumn fx:id="editColumn" prefWidth="85.0" text="Editar" />
            <TableColumn fx:id="deleteColumn" prefWidth="85.0" text="Excluir" />
        </columns>
    </TableView>
    <Button fx:id="btnVoltar" layoutX="20.0" layoutY="18.0" mnemonicParsing="false" onAction="#onClickVoltar" text="Voltar" />

</AnchorPane>
