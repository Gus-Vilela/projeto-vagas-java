<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cliente.controllers.VisualizarExperiencias">

    <TableView fx:id="experienciasTable" layoutX="20.0" layoutY="59.0" prefHeight="327.0" prefWidth="560.0">
        <columns>
            <TableColumn fx:id="competenciaColumn" prefWidth="200.0" text="Competência" />
            <TableColumn fx:id="experienciaColumn" prefWidth="200.0" text="Experiência em anos" />
            <TableColumn fx:id="editColumn" prefWidth="85.0" />
            <TableColumn fx:id="deleteColumn" prefWidth="85.0" />
        </columns>
    </TableView>
   <Button fx:id="btnVoltar" layoutX="20.0" layoutY="18.0" mnemonicParsing="false" onAction="#onClickVoltar" text="Voltar" />

</AnchorPane>
